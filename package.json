{
  "name": "uni-unocss",
  "type": "module",
  "version": "1.0.0-beta.4",
  "packageManager": "pnpm@10.7.1",
  "description": "采用更激进的语法在 UniApp 中使用 UnoCSS",
  "author": {
    "name": "sKy",
    "email": "319619193@qq.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/skiyee/uni-unocss#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skiyee/uni-unocss.git"
  },
  "bugs": {
    "url": "https://github.com/skiyee/uni-unocss/issues"
  },
  "keywords": [
    "uniapp",
    "unocss",
    "css"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "prepublishOnly": "pnpm build",
    "release": "bumpp"
  },
  "peerDependencies": {
    "unocss": "66.0.0"
  },
  "dependencies": {
    "@unocss/core": "66.0.0",
    "@unocss/preset-legacy-compat": "66.0.0",
    "@unocss/preset-wind3": "66.0.0",
    "unocss": "66.0.0"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.2.10",
    "@skiyee/eslint-config": "^0.1.0",
    "@types/node": "^24.3.1",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "tsdown": "^0.16.6",
    "typescript": "^5.9.2",
    "vite": "^5.2.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
